{
  "name": "tacnoman/dephpugger",
  "description": "Debug for php in terminal.",
	"scripts": {
    "test": "php vendor/bin/phpunit",
    "lint": "php vendor/bin/phpcs src"
	},
  "require": {
    "cboden/ratchet": "0.3.6",
    "symfony/console": "^3.2",
    "symfony/yaml": "^3.2"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "2.*",
    "phpunit/phpunit": "^7.1"
  },
  "autoload": {
    "psr-4" : {
      "Dephpug\\": "src/Dephpug",
      "Dephpug\\Console\\": "console/"
    }
  },
  "bin": ["bin/dephpugger"],
